FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    // actuatorDisk
    {
        name 	actuatorDisk;
        type	cellSet;
        action	new;
        source	boxToCell;
        sourceInfo
        {
            box (-0.05 -0.5 -0.2) (0.05 0.5 0.2)
        }
    }
    {
        name    actuatorDisk;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        sourceInfo
        {
            set actuatorDisk;
        }
    }

);

